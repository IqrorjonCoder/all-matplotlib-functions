   "source": [
    "fig, ax = plt.subplots()\n",
    "\n",
    "ax.scatter(x=np.random.random(100),\n",
    "           y=np.random.random(100),\n",
    "           s=np.random.randint(1,30,100),\n",
    "           marker='^')\n",
    "\n",
    "plt.show()"
   ],
